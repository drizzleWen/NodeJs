doctype
html
  head
  meta(charset='utf-8')
  title jade测试
  style.
    div{height:100px;width:200px;background:red;float:left;margin:10px auto}
    div.active{height:100px;width:200px;background:green;float:left;margin:10px auto}
  body
    -var i=0
    while i<=12
      if i%4==0&&i!=0
        div #{i++}
      else
        div.active #{i++}
